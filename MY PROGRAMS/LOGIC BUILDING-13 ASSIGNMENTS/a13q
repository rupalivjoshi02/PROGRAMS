/* Accept number from user and display below pattern.

Input:8
Output:2 4 6 8 10 12 14 16 

*/




#include<stdio.h>

void Display(int ino)
{
	int i=0,imult=0;

	if(ino<0)
	{
		ino=-ino;
	}

	for(i=1;i<=ino;i++)
	{
		imult=i*2;

		printf("%d ",imult);
	}
}

int main()
{
	int ivalue=0;

	printf("Enter the no.:");
	scanf("%d",&ivalue);

	Display(ivalue);

	return(0);
}
